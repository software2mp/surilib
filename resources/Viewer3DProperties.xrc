                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainLowQuality-32.png</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <option>1</option>
                                <object class="wxSlider" name="ID_SLIDER_QUALITY">
                                    <size>200,-1</size>
                                    <tooltip>Calidad del Terreno</tooltip>
                                    <style>wxSL_HORIZONTAL|wxSL_AUTOTICKS</style>
                                    <value>1</value>
                                    <min>1</min>
                                    <max>20</max>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                                <border>1</border>
                                <object class="wxStaticText" name="ID_QUAL_VALUE_LABEL">
                                    <label>1</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainHighQuality-32.png</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticText" name="wxID_STATIC">
                                    <label>Exageracion</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainLowExaggeration-32.png</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <option>1</option>
                                <object class="wxSlider" name="ID_SLIDER_EXAGERATION">
                                    <size>200,-1</size>
                                    <tooltip>Exageracion de Relieve</tooltip>
                                    <style>wxSL_HORIZONTAL|wxSL_AUTOTICKS</style>
                                    <value>1</value>
                                    <min>1</min>
                                    <max>20</max>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                                <border>1</border>
                                <object class="wxStaticText" name="ID_EXAG_VALUE_LABEL">
                                    <label>1 </label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainHighExaggeration-32.png</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticText" name="wxID_STATIC">
                                    <label>Vel. Camara</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainSlowCamera-32.png</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <option>1</option>
                                <object class="wxSlider" name="ID_SLIDER_CAMERA">
                                    <size>200,-1</size>
                                    <tooltip>Velocidad de Camara</tooltip>
                                    <style>wxSL_HORIZONTAL|wxSL_AUTOTICKS</style>
                                    <value>1</value>
                                    <min>1</min>
                                    <max>20</max>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                                <border>1</border>
                                <object class="wxStaticText" name="ID_VEL_CAM_VALUE_LABEL">
                                    <label>1 </label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
                                <border>5</border>
                                <object class="wxStaticBitmap" name="wxID_STATIC">
                                    <size>32,32</size>
                                    <bitmap>bitmaps/picture-TerrainFastCamera-32.png</bitmap>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
